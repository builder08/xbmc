SDKSTAGE=/opt/xbmc-bcm/buildroot/output/staging
XBMCPREFIX=/opt/xbmc-bcm/xbmc-bin
TARGETFS=/opt/xbmc-bcm/buildroot/output/target
TOOLCHAIN=/opt/xbmc-bcm/buildroot/output/host/usr/
BUILDROOT=/opt/xbmc-bcm/buildroot
USE_BUILDROOT=1
BASE_URL=http://mirrors.xbmc.org/build-deps/darwin-libs
TARBALLS_LOCATION=/opt/xbmc-tarballs
RETRIEVE_TOOL=/usr/bin/curl
RETRIEVE_TOOL_FLAGS=-Ls --create-dirs --output $(TARBALLS_LOCATION)/$(ARCHIVE)
ARCHIVE_TOOL=/bin/tar
ARCHIVE_TOOL_FLAGS=xf
JOBS=0
