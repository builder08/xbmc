<?xml version="1.0" encoding="utf-8" ?>
<settings>
  <section id="services">
    <category id="general">
      <group id="2">
        <setting id="services.zeroconf">
          <default>false</default>
        </setting>
      </group>
    </category>
    <category id="smb">
      <visible>false</visible>
    </category>
  </section>
  <section id="system">
    <category id="display">
      <group id="1">
        <setting id="videoscreen.fakefullscreen">
          <requirement negated="true">HAS_GL</requirement>
        </setting>
      </group>
    </category>
    <category id="audio">
      <group id="1">
        <setting id="audiooutput.audiodevice">
          <default>DIRECTSOUND:default</default>
        </setting>
      </group>
      <group id="3">
        <setting id="audiooutput.passthroughdevice">
          <default>DIRECTSOUND:default</default>
        </setting>
      </group>
    </category>
  </section>
  <section id="interface">
    <category id="regional">
      <group id="2">
        <setting id="locale.timezonecountry">
          <visible>false</visible>
        </setting>
        <setting id="locale.timezone">
          <visible>false</visible>
        </setting>
      </group>
    </category>
  </section>
</settings>
