					</focusedlayout>
					<content limit="$PARAM[limit]">$PARAM[content]</content>
				</control>
				<control type="button">
					<description>left Arrow</description>
					<left>-40</left>
					<top>90</top>
					<width>30</width>
					<height>40</height>
					<texturefocus>arrow-big-left.png</texturefocus>
					<texturenofocus>arrow-big-left.png</texturenofocus>
					<onclick>Control.Move($PARAM[ID],-1)</onclick>
					<visible>Control.HasFocus($PARAM[ID]) + Container($PARAM[ID]).HasPrevious</visible>
